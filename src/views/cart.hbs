<div>
        Carrito
        <ul>
        {{#each payload}}
            <li>{{this.title}}</li>
            <li>{{this.description}}</li>
            <li>{{this.code}}</li>
            <li>{{this.price}}</li>
            <li>{{this.status}}</li>
            <li>{{this.stock}}</li>
            <li>{{this.category}}</li>
            <button onclick="addToCart(`{{this._id}}`)">Agregar al carrito</button>
            <hr>
        {{/each}}
        </ul>
    </div>