<h1>Productos</h1>
<div>
    <ol>
        {{#each renProducts}}
        <li>
            <ul>
                <li>{{this.title}}</li>
                <li>{{this.description}}</li>
                <li>{{this.code}}</li>
                <li>{{this.price}}</li>
                <li>{{this.status}}</li>
                <li>{{this.stock}}</li>
                <li>{{this.category}}</li>
                {{#if this.thumbnails}}
                    <li>{{this.thumbnails}}</li>
                {{/if}}
                
            </ul>
        </li>
        <br>
    {{/each}}
    </ol>
</div>

<script src="/socket.io/socket.io.js"></script>
<script src="/js/home.js"></script>